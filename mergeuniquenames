def unique_names(names1, names2):
    Result = ""
    for name in names1:
        Result = Result + name
    for name in names2:
        if name not in names1:
            Result = Result + name
        
    return Result

if __name__ == "__main__":
    names1 = ["Ava", "Emma", "Olivia"]
    names2 = ["Olivia", "Sophia", "Emma"]
    print(unique_names(names1, names2)) # should print Ava, Emma, Olivia, Sophia
